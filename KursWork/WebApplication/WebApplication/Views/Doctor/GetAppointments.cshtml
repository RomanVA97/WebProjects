<div id="table" class="col-md-9">
<table class="table table-hover">
<tr>
<td class="active">ФИО пациента</td>
<td class="success">Время</td>
<td class="warning">Номер паспорта</td>
</tr>
@foreach (var item in ViewBag.Items)
{
<tr ondblclick="OnClick(@item.Id)">
<td class="active">@item.FIO</td>
<td class="success">@item.Time</td>
<td class="warning">@item.PasportNumber</td>
</tr>
}
</table>
</div>
@section scripts{
    <script type="text/javascript">
        function OnClick(Id)
        {
            $.ajax({
                type: 'GET',
                url: '@Url.Action("TakeAPatient")?id='+Id,
                success: function (data) {
                    $('#workArea').replaceWith(data);
                }
            });
            
        }
    </script>
}